{
  "name": "proj-ma",
  "displayName": "橡木山",
  "publisher": "your-publisher",
  "description": "VS Code / Cursor 扩展，集成 React + Ant Design Webview",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "projMa",
          "title": "橡木山",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "projMa": [
        {
          "type": "webview",
          "id": "projMa.view",
          "name": "橡木山"
        }
      ]
    }
  },
  "scripts": {
    "build": "npm run build:webview && npm run build:extension",
    "build:extension": "vite build -c vite.config.extension.ts",
    "build:webview": "npm --prefix webview run build",
    "watch": "npm-run-all --parallel watch:*",
    "watch:ext": "vite build -c vite.config.extension.ts --watch",
    "watch:webview": "npm --prefix webview run build:watch",
    "dev": "npm run watch",
    "vscode:prepublish": "npm run build",
    "package": "vsce package --no-dependencies"
  },
  "devDependencies": {
    "@types/node": "^20.19.13",
    "@types/vscode": "^1.84.0",
    "@vscode/vsce": "^3.6.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.4.0",
    "vite": "^6.0.7"
  },
  "dependencies": {
    "axios": "^1.11.0"
  }
}