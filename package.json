{
  "name": "proj-ma",
  "displayName": "Proj MA",
  "publisher": "your-publisher",
  "description": "VS Code / Cursor 扩展，集成 React + Ant Design Webview",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "projMa",
          "title": "Proj MA",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "projMa": [
        {
          "type": "webview",
          "id": "projMa.view",
          "name": "Proj MA"
        }
      ]
    }
  },
  "scripts": {
    "build": "npm run build:webview && tsc -p .",
    "build:webview": "npm --prefix webview run build",
    "watch": "npm-run-all --parallel watch:*",
    "watch:ext": "tsc -watch -p .",
    "watch:webview": "npm --prefix webview run build:watch",
    "dev": "npm run watch",
    "vscode:prepublish": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/vscode": "^1.84.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.4.0"
  }
}